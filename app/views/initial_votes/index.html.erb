<p style="color: green"><%= notice %></p>

<h1>Initial votes</h1>

<div id="initial_votes">
  <% @initial_votes.each do |initial_vote| %>
    <%= render initial_vote %>
    <p>
      <%= link_to "Show this initial vote", initial_vote %>
    </p>
  <% end %>
</div>

<%= link_to "New initial vote", new_initial_vote_path %>
